{
  "name": "ralph-extension",
  "displayName": "Ralph Extension",
  "description": "A custom Cursor extension",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ralph-extension.typeHelloWorld",
        "title": "Ralph Loop"
      },
      {
        "command": "ralph-extension.stopLoop",
        "title": "Stop Loop",
        "icon": "$(stop)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "npm run compile && vsce package"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}
